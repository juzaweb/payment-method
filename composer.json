{
    "name": "juzaweb/payment-method",
    "type": "juzaweb-plugin",
    "description": "Payment method plugin for Juzaweb CMS",
    "extra": {
        "juzaweb": {
            "name": "juzaweb/payment-method",
            "domain": "payment_method",
            "version": "1.0",
            "providers": [
                "Juzaweb\\PaymentMethod\\Providers\\PaymentMethodServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\PaymentMethod\\": "src/"
        }
    },
    "require": {
        "league/omnipay": "^3",
        "omnipay/paypal": "^3.0"
    }
}
